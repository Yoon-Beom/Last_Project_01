<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.member">
<!--<resultMap type="java.util.HashMap" id="memResult">-->	
	<resultMap type="memberVO" id="memResult">
		<result property="member_id" column="member_id" />
		<result property="member_pwd" column="member_pwd" />
		<result property="member_name" column="member_name" />
		<result property="member_birth" column="member_birth" />
		<result property="member_phone" column="member_phone" />
		<result property="member_email" column="member_email" />
		<result property="member_gender" column="member_gender" />
		<result property="member_address" column="member_address" />
		<result property="member_joinDate" column="member_joinDate" />
	</resultMap>
	<select id="selectAllMemberList" resultMap="memResult">
		<![CDATA[
			select * from t_member order by member_joinDate desc
		]]>
	</select>
	<insert id="insertMember" parameterType="memberVO">
		<![CDATA[		
			insert into t_member(member_NO, member_id, member_pwd, member_name,
				member_birth, member_phone, member_email, member_gender,
				member_address) 
				values(seq_member.nextval, #{member_id}, #{member_pwd}, #{member_name},
				#{member_birth}, #{member_phone},#{member_email}, #{member_gender},#{member_address})
		]]>
	</insert>
</mapper>